#ifndef ROOTSETUPWRAPPER_H
#define ROOTSETUPWRAPPER_H

#include <cstddef>
#include <map>
#include "TString.h"
#include "Setup.h"

class RootSetupWapper{
  
 public:
  RootSetupWrapper(Setup* ){}
  ~RootSetupWrapper() {}
  
 private:
  
  bool isInit=false;
  //key is CellID
  std::map<int, TString> assemblyID;
  std::map<int, int>     ROunit;
  std::map<int, int>     ROchannel;
  std::map<int, int>     Board;
  //Inverse mapping
  std::map< std::pair<int, int>, int> CellIDfromRO;

  class friend Setup;
  ClassDef(Setup,1)
};


#endif
